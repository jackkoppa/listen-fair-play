{
  "name": "@listen-fair-play/rss-retrieval-lambda",
  "version": "0.0.1",
  "description": "Retrieve RSS feeds and download audio files",
  "main": "dist/retrieve-rss-feeds-and-download-audio-files.js",
  "module": "dist/retrieve-rss-feeds-and-download-audio-files.js",
  "type": "module",
  "files": [
    "dist"
  ],
  "scripts": {
    "build:dev": "dotenvx run -f ../../../.env.dev -- tsc && pnpm __prepare-for-aws",
    "run:local": "dotenvx run -f ../../../.env.local -- tsx lamdas/retrieve-rss-feeds-and-download-audio-files.ts",
    "run:dev-s3": "dotenvx run -f ../../../.env.dev -- tsx lamdas/retrieve-rss-feeds-and-download-audio-files.ts",
    "test": "vitest run",
    "__prepare-for-aws": "../../../scripts/pnpm-deploy-with-versions-fix.sh @listen-fair-play/rss-retrieval-lambda"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "catalog:",
    "@aws-sdk/credential-provider-sso": "catalog:",
    "@aws-sdk/s3-request-presigner": "catalog:",
    "@listen-fair-play/constants": "workspace:*",
    "@listen-fair-play/s3-utils": "workspace:*",
    "@listen-fair-play/utils": "workspace:*",
    "axios": "^1.6.7",
    "flexsearch": "catalog:",
    "fluent-ffmpeg": "^2.1.2",
    "fs-extra": "catalog:",
    "openai": "^4.28.0",
    "srt-parser-2": "^1.2.3",
    "xml2js": "^0.6.2"
  },
  "devDependencies": {
    "@listen-fair-play/types": "workspace:*",
    "@types/fluent-ffmpeg": "^2.1.24",
    "@types/fs-extra": "catalog:",
    "@types/node": "^20.0.0",
    "@types/xml2js": "^0.4.14",
    "tsx": "^4.7.0",
    "typescript": "catalog:",
    "vitest": "^1.6.1"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}